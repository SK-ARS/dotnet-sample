@{
    int strClone = ViewBag.IsClone;
    int strRevise = ViewBag.IsRevise;
    int AppRevId = ViewBag.AppRevId;
}
<div class="row pt-8 pr-8">
    <div class="col-lg-6 col-md-6 col-sm-12">
        <span class="title">Supplementary Information</span>
    </div>
    <div class="col-lg-6 col-md-6 col-sm-12" id="table-head" style="justify-content: end;">

        <span id="table-head">
            @if (strClone == 1 || strRevise == 1)
            {
                <div class="button main-button mr-3 pr-3">
                    <button id="btnSaveSupplementary" class="btn btn-outline-primary btn-normal" role="button" aria-pressed="true" style="display:none;">
                        Save
                    </button>
                    <button id="btnEditSupplementary" class="btn btn-outline-primary btn-normal" role="button" aria-pressed="true">
                        Edit
                    </button>
                </div>
            }
            <span id="IDviewsupplimentaryinfo">
                <img src="~/Content/assets/images/up-chevlon-filter.svg" width="15" id="chevlon-up-icon2"
                     style="display: none; cursor: pointer;">
                <img src="~/Content/assets/images/down-chevlon-filter.svg" width="15" id="chevlon-down-icon2" style="cursor: pointer;">
            </span>
        </span>
    </div>
</div>
<hr style="margin-right: 3rem;">
<div class="mt-4 main-entry">
    <div class="row" id="viewsupplimentary" style="display: none;">
        @{
            Html.RenderAction("ViewSupplementary", "Application", new { appRevisionId = AppRevId, isClone = @ViewBag.IsClone, isRevise = @ViewBag.IsRevise });//20192
        }
    </div>
</div>
<script src="~/Scripts/CspJs/Application\PartialView/_ViewApplicationSupplimentary.js?V@(Session["ProjectVersion"])"></script>
