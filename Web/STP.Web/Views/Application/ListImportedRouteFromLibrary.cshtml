@model List<STP.Domain.Routes.AppRouteList>

@{

    var modelcnt = 0;
    modelcnt = Model.Count;
}

<style>
    #mid_row {
        justify-content: flex-end;
    }
  .DivCSSLisTImported {
        margin-top: 38.25rem !important;
        position:relative;
    }
</style>
@Html.Hidden("ModelCnt", modelcnt)
<div id="ShowList">
    @{
        if (Model.Count != 0 || ViewBag.AuthoMov_VersionID != "0" || ViewBag.AuthoMov_CONT_REF != "0")
        {
            <div>
                @if (ViewBag.IsNEN == true)
                {
                    <div class="pt-2 pb-3 edit-normal">
                        Listed below, are the routes you have defined for your notification.
                    </div>
                }
                else
                {
                    <div class="pt-2 pb-3 edit-normal">
                        Listed below, are the routes you have defined for your application.
                    </div>
                }

                <div id="tblroutelist" class="pr-3">
                    <div class="main-table mt-0 pt-0">
                        <table>
                            <thead>
                                @{
                                    if (ViewBag.IsNEN == true)
                                    {
                                        <tr>
                                            @if (ViewBag.IsNEN_authoMov == true)
                                            {

                                                <th>Route parts</th>
                                            }
                                            else
                                            {
                                                <th>Start Address</th>
                                                <th>End Address</th>
                                            }
                                            @if (ViewBag.IsNEN_authoMov == true)
                                            {
                                                <th>Route Type</th>
                                            }
                                            else
                                            {
                                                <th>Route Status</th>

                                            }
                                            <th>Route Description</th>
                                            @if (ViewBag.IsNEN_authoMov == false)
                                            {
                                                <th></th>

                                                <th></th>
                                                <th></th>
                                            }
                                        </tr>
                                    }
                                    else
                                    {
                                        <tr>
                                            <th>Route Parts</th>

                                            <th>Route Type</th>

                                            <th>Route Description</th>

                                            @if (ViewBag.AuthoMov_VersionID == "0" && ViewBag.AuthoMov_CONT_REF == "0")
                                            {
                                                <th></th>
                                            }
                                        </tr>
                                    }
                                }
                            </thead>
                            <tbody id="sortable">
                                @{
                                    if (ViewBag.AuthoMov_VersionID != "0" || ViewBag.AuthoMov_CONT_REF != "0")
                                    {
                                        if (ViewBag.AuthoMov_VersionID != "0")
                                        {
                                            for (int j = 0; j < Model.Count; j++)
                                            {
                                                <tr class="bgColor3" id='@j' data-id="@Model[j].RouteID" data-type=" @Model[j].RouteType">
                                                    <td class="text-color1">
                                                        <span class="green link" id="span1-routeview" data-RouteID="@Model[j].RouteID" data-RouteType="@Model[j].RouteType" data-var1=true>@Model[j].RouteName</span>
                                                    </td>
                                                    <td class="text-color1">@Model[j].RouteType</td>
                                                    <td class="text-color1">@Model[j].RouteDescription</td>
                                                </tr>

                                            }
                                        }
                                        else
                                        {
                                            for (int j = 0; j < Model.Count; j++)
                                            {
                                                <tr class="bgColor3" id='@j' data-id="@Model[j].RouteID" data-type=" @Model[j].RouteType">
                                                    <td class="text-color1">
                                                        <span class="green link" id="span2-routeview" data-RouteID="@Model[j].RouteID" data-RouteType="@Model[j].RouteType" data-var1=false>@Model[j].RouteName</span>
                                                    </td>
                                                    <td class="text-color1">@Model[j].RouteType</td>
                                                    <td class="text-color1">@Model[j].RouteDescription</td>

                                                </tr>
                                            }

                                        }
                                        if (Model.Count == 0)
                                        {
                                            <tr class="bgColor3">
                                                <td colspan="4" class="text-color1"><span style="color: red">Result not found.</span></td>
                                            </tr>
                                        }

                                    }

                                    if (ViewBag.AuthoMov_VersionID == "0" && ViewBag.AuthoMov_CONT_REF == "0")
                                    {
                                        for (int i = 0; i < Model.Count; i++)
                                        {

                                            if (ViewBag.IsNEN == true)
                                            {
                                                <tr class="bgColor3" id='@i' data-id="@Model[i].RouteID" data-type="@Model[i].RouteType">
                                                    @if (ViewBag.IsNEN_authoMov == true)
                                                    {
                                                        <td class="text-color1">
                                                            <span class="green link" id="span3-routeview" data-RouteID="@Model[i].RouteID" data-RouteType="@Model[i].RouteType" data-var1=true>@Model[i].RouteName</span>
                                                        </td>
                                                    }
                                                    else
                                                    {
                                                        if (Model[i].FromAddress.Length > 30)
                                                        {
                                                            <td class="text-color1" title="@Model[i].FromAddress" style="width:230px;">@Model[i].FromAddress.Substring(0, 30)...</td>
                                                        }
                                                        else
                                                        {
                                                            <td class="text-color1" title="@Model[i].FromAddress" style="width:230px;">@Model[i].FromAddress</td>
                                                        }

                                                        if (Model[i].ToAddress.Length > 30)
                                                        {
                                                            <td class="text-color1" title="@Model[i].ToAddress" style="width:230px;">@Model[i].ToAddress.Substring(0, 30)...</td>
                                                        }
                                                        else
                                                        {
                                                            <td class="text-color1" title="@Model[i].ToAddress" style="width:230px;">@Model[i].ToAddress</td>
                                                        }
                                                    }
                                                    @{string rtname = @Model[i].RouteName;
                                                        string rtname1 = rtname.Replace("'", ""); }
                                                    @if (ViewBag.IsNEN_authoMov == true)
                                                    {
                                                        <td class="text-color1"><span>Planned</span></td>
                                                    }
                                                    else
                                                    {
                                                        <td class="text-color1"><span id="status_@Model[i].RouteID">@Model[i].NENRouteStatus</span></td>
                                                    }
                                                    @if (Model[i].RouteDescription.Length > 40)
                                                    {
                                                        if (Model[i].RouteDescription.Length > 1010)
                                                        {
                                                            <td class="text-color1" title="@Model[i].RouteDescription.Substring(0, 1010)......." style="width:230px;">@Model[i].RouteDescription.Substring(0, 40)...</td>
                                                        }
                                                        else
                                                        {
                                                            <td class="text-color1" title="@Model[i].RouteDescription" style="width:230px;">@Model[i].RouteDescription.Substring(0, 40)...</td>
                                                        }

                                                    }
                                                    else
                                                    {
                                                        <td class="text-color1" style="width: 230px;">@Model[i].RouteDescription</td>
                                                    }
                                                    @if (ViewBag.IsNEN_authoMov == false)
                                                    {
                                                        <td class="text-color1">
                                                            <span id="mid_row">
                                                                <button class="btn btn-outline-primary btn-normal" id="btn1-viewroute" data-RouteID="@Model[i].RouteID" data-planned='planned' data-NENRouteStatus="@Model[i].NENRouteStatus">
                                                                    VIEW ROUTE
                                                                </button>
                                                            </span>
                                                        </td>

                                                        <td class="text-color1">
                                                            <a class="text-decoration-link" style="cursor:pointer;" id="edit-route1" data-RouteID="@Model[i].RouteID" data-rtname1="@rtname1" data-planned='planned' name="@Model[i].RouteName">
                                                                <img src="/Content/assets/images/edit-icon.svg" width="20" data-toggle="tooltip" data-placement="right" title="Edit">
                                                            </a>
                                                        </td>
                                                    }
                                                </tr>
                                            }
                                            else
                                            {
                                                <tr class="bgColor3" id='@i' data-id="@Model[i].RouteID" data-type="@Model[i].RouteType">


                                                    <td class="text-color1">
                                                        @if (ViewBag.IsCandidateModify == null)
                                                        {

                                                            <span class="green link" id="displaySoRouteDescMap" data-RouteID="@Model[i].RouteID" data-RouteType="@Model[i].RouteType">@Model[i].RouteName</span>
                                                        }
                                                        else
                                                        {
                                                            <span class="green link" id="displayCandidateRouteDescMap" data-RouteID="@Model[i].RouteID" data-RouteType="@Model[i].RouteType">@Model[i].RouteName</span>
                                                        }
                                                    </td>
                                                    <td class="text-color1">
                                                        @Model[i].RouteType
                                                    </td>


                                                    <td class="text-color1">
                                                        @Model[i].RouteDescription
                                                    </td>


                                                    <td class="text-color1">
                                                        @if (ViewBag.IsCandidateModify == null)
                                                        {
                                                            <button style="width:auto;margin-left:8px" id="addtolib" data-RouteID="@Model[i].RouteID" data-namer="@Model[i].RouteName" data-RouteType="@Model[i].RouteType" name="@Model[i].RouteName" class="tdbuttonauto tdright btngrad btnrds btnbdr vehicleactions" aria-hidden="true">Add to route library</button>
                                                        }
                                                        @if (ViewBag.IsCandidateModify == null || (ViewBag.IsCandidateModify == true && ViewBag.CheckerStatus != 301003))
                                                        {
                                                            if (ViewBag.AgreedSO == 0)
                                                            {
                                                                string rtname = @Model[i].RouteName;
                                                                string rtname1 = rtname.Replace("'", "");
                                                                <button data-RouteID="@Model[i].RouteID" data-RouteType="@Model[i].RouteType" id="btn_del_route" class="tdbutton tdright btngrad btnrds btnbdr vehicleactions" name="@Model[i].RouteName" aria-hidden="true" data-icon="&#xe0bc;">Delete</button>
                                                                <button data-RouteID="@Model[i].RouteID" data-rtname1="@rtname1'" data-RouteType="@Model[i].RouteType" name="@Model[i].RouteName" id="btn_edit_route" class="tdbutton tdright btngrad btnrds btnbdr vehicleactions" aria-hidden="true" data-icon="&#xe000;">Edit</button>

                                                            }
                                                            else if (ViewBag.AgreedSO == 2)
                                                            {
                                                                <button data-RouteID=" @Model[i].RouteID" data-RouteType="@Model[i].RouteType" id="btn-DeleteSelectedRoute" class="tdbutton tdright btngrad btnrds btnbdr vehicleactions" name="@Model[i].RouteName" aria-hidden="true" data-icon="&#xe0bc;">Delete</button>
                                                            }
                                                        }
                                                    </td>

                                                </tr>

                                            }

                                        }
                                    }
                                }

                            </tbody>
                        </table>
                    </div>
                </div>

                @if ((ViewBag.IsCandidateModify == null || ViewBag.IsCandidateModify == true) && ViewBag.IsNEN == false)
                {
                    <div id="reordering">
                        <button id="sort" class="moveupdownsave btngrad btnrds btnbdr" type="button" aria-hidden="true" class="btnautowidth next btngrad btnrds btnbdr">Reorder route parts</button>
                        <button id="sort11" class="moveupdownsave btngrad btnrds btnbdr" type="button" aria-hidden="true" class="btnautowidth next btngrad btnrds btnbdr">Finish reordering</button>
                        <span id="information" style="width: auto; margin-top: 8px;">Click and drag route part to reorder.</span>
                        <button class="btn_reg_back next btngrad btnrds btnbdr" id="btn_cancel" aria-hidden="true" data-icon="î„™" type="button">Cancel</button>
                    </div>
                }
            </div>
        }
        else if (ViewBag.IsNEN == true)
        {

            <h4 style="display: block;" id="h4_blank_header">Route details not found.</h4>
            <br />
        }
        else
        {

            <h4 style="display: block;" id="h4_blank_header">Please add one or more route parts to your application by making a selection in the left menu.</h4>
            <br />
        }
    }
</div>


