                }
                <div class="row pb-3">
                    <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                        Load
                    </div>
                    <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                        @Model.LoadDescription
                    </div>
                </div>
                <div class="row pb-3">
                    <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                        Application Date
                    </div>
                    <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                        @Model.ApplicationDate
                    </div>
                </div>
                <div class="row pb-3">
                    <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                        Movement Date From
                    </div>
                    <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                        @{CultureInfo ukCulture = new CultureInfo("en-GB");}
                        @Convert.ToDateTime(Model.MovementDateFrom).ToString("dd/MM/yyyy", ukCulture.DateTimeFormat)
                    </div>
                </div>
                <div class="row pb-3">
                    <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                        Movement Date To
                    </div>
                    <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                        @{CultureInfo ukCulture1 = new CultureInfo("en-GB");}
                        @Convert.ToDateTime(Model.MovementDateTo).ToString("dd/MM/yyyy", ukCulture1.DateTimeFormat)
                    </div>
                </div>
                @{
                    if (ViewBag.hideflag != true)
                    {
                        if (ViewBag.Vr1ApplStatus != 308002)
                        {
                            if (Model.VR1Number != "0")
                            {
                                <div class="row pb-3">
                                    <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                                        VR-1 Number
                                    </div>
                                    <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                                        NH: @Model.VR1Number
                                    </div>
                                </div>
                            }
                            if (Model.VApprDate != "0")
                            {
                                <div class="row pb-3">
                                    <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                                        VR-1 Approval Date
                                    </div>
                                    <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                                        @Model.VApprDate
                                    </div>
                                </div>
                            }
                        }
                    }
                }
                <div class="row pb-3">
                    <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                        Number of Movements
                    </div>
                    <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                        @Model.NoOfMovements
                    </div>
                </div>
                <div class="row pb-3">
                    <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                        Maximum Pieces per Load
                    </div>
                    <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                        @Model.MaxPiecesPerLoad
                    </div>
                </div>

                @{ if (ViewBag.reduceddetailed == true || Model.ReducedDetails == 1)
                    {
                        <div class="row pb-3">
                            <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                                Overall Width
                            </div>
                            <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                                @Model.OverallWidth m
                            </div>
                        </div>
                        <div class="row pb-3">
                            <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                                Overall Height
                            </div>
                            <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                                @Model.OverallHeight m
                            </div>
                        </div>
                        <div class="row pb-3">
                            <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                                Overall Length
                            </div>
                            <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                                @Model.OverallLength m
                            </div>
                        </div>
                        <div class="row pb-3">
                            <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                                Gross Weight
                            </div>
                            <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                                @Model.GrossWeight kg
                            </div>
                        </div>


                        if (Model.ReducedDetails == 1)
                        {
                            <div class="row pb-3">
                                <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                                    Vehicle Description
                                </div>
                                <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                                    @Model.VehicleDescription
                                </div>
                            </div>

                        }
                    }
                }
                <div class="row pb-3">
                    <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                        Notes to be included with the Application
                    </div>
                    <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                        @MvcHtmlString.Create(Html.Encode(Model.ApplicationNotes).Replace(Environment.NewLine, "<br />"))

                    </div>
                </div>

                @{
                    if (@Model.ContactName != null && @Model.ContactName != "")
                    {
                        if (ViewBag.hideflag != true)
                        {
                            <div class="row pb-3">
                                <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                                                    <b>Applying on Behalf of</b>
                                </div>

                            </div>
                            if (!string.IsNullOrEmpty(Model.ContactName))
                            {
                                <div class="row pb-3">
                                    <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                                        Contact name
                                    </div>
                                    <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                                        @Model.ContactName
                                    </div>
                                </div>
                            }
                            if (!string.IsNullOrEmpty(Model.HaulierOrgName))
                            {
                                <div class="row pb-3">
                                    <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                                        Organisation name
                                    </div>
                                    <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                                        @Model.HaulierOrgName
                                    </div>
                                </div>
                            }
                            if (!string.IsNullOrEmpty(Model.HaulierAddress1) || !string.IsNullOrEmpty(Model.HaulierAddress2) || !string.IsNullOrEmpty(Model.HaulierAddress3)
