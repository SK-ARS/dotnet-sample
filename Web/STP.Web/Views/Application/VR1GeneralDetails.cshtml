                                <div class="row pb-3">
                                    <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                                        Postcode
                                    </div>
                                    <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                                        @Model.OnBehalfOfPostCode
                                    </div>
                                </div>
                            }
                            else if (!string.IsNullOrEmpty(Model.HaulierPostCode))
                            {
                                <div class="row pb-3">
                                    <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                                        Postcode
                                    </div>
                                    <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                                        @Model.HaulierPostCode
                                    </div>
                                </div>
                            }
