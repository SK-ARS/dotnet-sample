                         new VR1SubMovementClass {SubMovementClass = "241004", Name = "STGO AIL Cat-2"},
                         new VR1SubMovementClass {SubMovementClass = "241005", Name = "STGO AIL Cat-3"},
                     };

        ViewBag.SubCat = new SelectList(SubCatList, "SubMovementClass", "Name");
        @Html.HiddenFor(m => m.ApplicationRevisionId)
        @Html.Hidden("AnalysisID", TempData["analysisID"])
        @Html.Hidden("proID", TempData["proID"])
        @Html.Hidden("Project_ID", Project_ID)
