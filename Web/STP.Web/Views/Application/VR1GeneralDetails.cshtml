                                    <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                                        Country
                                    </div>
                                    <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                                        @Model.Country
                                    </div>
                                </div>
