
@using System.Web.Optimization
@using STP.Domain
@using System.Globalization
@using STP.Domain.Applications
@model STP.Domain.Applications.ApplyForVR1

@{
    long ApplrevisionId = 0;
    long Project_ID = 0;
    int IsHistory = ViewBag.IsHistory;
    if (ViewBag.ProjectID != 0)
    {
        Project_ID = ViewBag.ProjectID;
    }
    if (ViewBag.revisionId != 0)
    {
        ApplrevisionId = ViewBag.revisionId;
    }
    if (Model == null)
    {
    }
    else
    {
        ViewBag.Title = "VR1GeneralDetails";

        var SubCatList = new List<VR1SubMovementClass>{
                         new VR1SubMovementClass {SubMovementClass = "0" ,Name = "--Please select--"},
