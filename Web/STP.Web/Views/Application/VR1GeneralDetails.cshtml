                            {
                                <div class="row pb-3">
                                    <div class="col-sm-4 col-md-6 col-lg-3 text-normal">
                                        Address
                                    </div>
                                    <div class="col-sm-4 col-md-5 col-lg-6 input-field edit-normal pt-0  pb-0 pl-0 pr-0">
                                        @{
                                            if (!string.IsNullOrEmpty(Model.HaulierAddress1))
                                            {
                                                @Model.HaulierAddress1 <br />
                                            }
                                            if (!string.IsNullOrEmpty(Model.HaulierAddress2))
                                            {
                                                @Model.HaulierAddress2 <br />
                                            }
                                            if (!string.IsNullOrEmpty(Model.HaulierAddress3))
                                            {
                                                @Model.HaulierAddress3 <br />
                                            }
                                            if (!string.IsNullOrEmpty(Model.HaulierAddress4))
                                            {
                                                @Model.HaulierAddress4 <br />
                                            }
                                            if (!string.IsNullOrEmpty(Model.HaulierAddress5))
                                            {
