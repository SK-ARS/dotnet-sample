                                <table>
                                    <tr>
                                        <td>
                                            <div style="width: 2rem">
                                                <img src="../../Content/assets/images/route-start-icon.svg" width="15" alt="vehicle-2">
                                            </div>
                                        </td>
                                        <td>
                                            <div>
                                                <div style="width: auto" class="pl-0">
                                                    <p>
                                                        @{
                                                            if (Model.JourneyFromToSummary.From != null)
                                                            {
                                                                if (!string.IsNullOrEmpty(Model.ESDALReferenceNumber.Mnemonic) && Model.ESDALReferenceNumber.MovementProjectNumber != "0" && !string.IsNullOrEmpty(Model.ESDALReferenceNumber.MovementVersion.Value) &&
                                                            Model.ESDALReferenceNumber.MovementVersion.Value != "0")
                                                                {
                                                                    if (Model.JourneyFromToSummary.From.Contains("##**##"))
                                                                    {
                                                                        string FromBefore = Model.JourneyFromToSummary.From.Split(new string[] { "##**##" }, StringSplitOptions.None)[0];
                                                                        string FromAfter = Model.JourneyFromToSummary.From.Split(new string[] { "##**##" }, StringSplitOptions.None)[1];
                                                                        <strike> @FromBefore</strike> <b><u>@FromAfter</u></b>
                                                                    }
                                                                    else
                                                                    {
                                                                        <span>@Model.JourneyFromToSummary.From</span>
                                                                    }
                                                                }
                                                                else
                                                                {
                                                                    if (Model.JourneyFromToSummary.From.Contains("##**##"))
                                                                    {
                                                                        string FromBefore = Model.JourneyFromToSummary.From.Split(new string[] { "##**##" }, StringSplitOptions.None)[0];
                                                                        string FromAfter = Model.JourneyFromToSummary.From.Split(new string[] { "##**##" }, StringSplitOptions.None)[1];
                                                                        <strike> @FromBefore</strike> <b><u>@FromAfter</u></b>
                                                                    }
                                                                    else
                                                                    {
                                                                        <span>@Model.JourneyFromToSummary.From</span>
                                                                    }
                                                                }
                                                            }
                                                        }
                                                    </p>
                                                </div>
                                            </div>
                                        </td>
                                    </tr>
                                </table>
                            </div>
                            <div>
                                <div style="width: 2rem;padding-left: 4px; margin-top: -11px;">
                                    <p style="display: flex;flex-direction: column; padding-top: -4px;">
                                        <span style="height: 6px;">.</span>
