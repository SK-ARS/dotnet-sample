 <div  class="main-table" id="div_session_length_list">
     <div id="div_tbl_grid" runat="server">
         <table>
             <thead>
                 <tr>
                     <th class="first-column-padding">@Resources.Resource.Organisation</th>
                     <th>@Resources.Resource.TotalSession</th>
                     @if (ViewBag.UserType == 696001)
                     {
                         <th>@Resources.Resource.TotalNotificationSent</th>
                     }
                     else
                     {
                         <th>@Resources.Resource.TotalNotificationReceive</th>
                     }
                 </tr>
             </thead>
             <tbody>
                 @for (int count = 0; count < Model.Count; count++)
                 {
                     <tr>
                         <td class="text-color1">@Model[count].OrganisationName</td>
                         <td class="text-color1">@Decimal.ToInt32(Model[count].TotalSession)</td>
                         <td class="text-color1">@Decimal.ToInt32(Model[count].TotalNotification)</td>
                     </tr>
                 }
             </tbody>
         </table>
         @if (Model.Count <= 0)
         {
             <div class="redcolor aligncenter">@Resources.Resource.NoRecordFound</div>
         }
     </div>
    <div id="div_export_list">
        <button id="btnExport" class="btn outline-btn-primary SOAButtonHelper ml2">@Resources.Resource.Export.ToUpper()</button>
    </div>
</div>
