    @using STP.Common.Constants
    @{
        bool notify = false;
        string helpdesk_back = "false";
        string daysRemaining = string.Empty;
        string Htmlfilepath = string.Empty;
        Htmlfilepath = System.Configuration.ConfigurationManager.AppSettings["HtmlHelper"];
        string userName = string.Empty;
        int portalType = 0;
        bool isAdmin = false;
        bool isHelpEdit = false;
        int LatestNewsTimeInterval = 0;
        LatestNewsTimeInterval = Convert.ToInt16(System.Configuration.ConfigurationManager.AppSettings["LatestNewsTimeInterval"]);
        var sessionValues = (STP.Domain.SecurityAndUsers.UserInfo)Session["UserInfo"];
        var latestNews = Session["NewNewsCount"];
        var isRead = Session["IsRead"];
        DateTime LatestDateTime = new DateTime();
        if (Session["PreviousNewsTime"] != null)
        {
            LatestDateTime = Convert.ToDateTime(Session["PreviousNewsTime"]);
        }



        if (Session["UserInfo"] != null)
        {
            userName = sessionValues.UserName;
            portalType = sessionValues.UserTypeId;
            if (portalType == 696006 && sessionValues.IsAdmin == 1)
            {
                isAdmin = true;
            }
            if (portalType == 696006 || sessionValues.IsAdmin == 1)
            {
                isHelpEdit = true;
            }
        }
        if (sessionValues != null)
        {
            if (sessionValues.HelpdeskRedirect == "true")
            {
                ViewBag.HelpdeskDistRedirect = "true";
            }
            else
            {
                ViewBag.HelpdeskDistRedirect = "false";
            }
        }

        if (ViewBag.DaysToExpire != null)
        {
            notify = true;
            daysRemaining = ViewBag.DaysToExpire;
        }


        if (ViewBag.HelpdeskDistRedirect != null)
        {
            helpdesk_back = ViewBag.HelpdeskDistRedirect;
        }

        @Html.Hidden("Notification", notify)
        @Html.Hidden("DaysRemaining", daysRemaining)
        @Html.Hidden("PortalType", portalType)
        @Html.Hidden("HtmlFilePath", Htmlfilepath)
        @Html.Hidden("helpdesk_back", helpdesk_back) switch (portalType)
        {
            case 696001:
                ViewBag.PortalType = Constants.Haulier;
                break;
            case 696002:
                ViewBag.PortalType = Constants.Police;
                break;
            case 696003:
                ViewBag.PortalType = Constants.OPS;
                break;
            case 696004:
                ViewBag.PortalType = Constants.MIS;
                break;
            case 696005:
                ViewBag.PortalType = Constants.PUBLIC;
                break;
            case 696006:
                ViewBag.PortalType = Constants.HelpDesk;
                break;
            case 696007:
                ViewBag.PortalType = Constants.SOA;
                break;
            case 696008:
                ViewBag.PortalType = Constants.SORT;
                break;
            default:
                ViewBag.PortalType = "";
                break;
        }

        bool firstUser = false;
        if (Session["RedirectToChangePwd"] != null && (bool)Session["RedirectToChangePwd"])
        {
            firstUser = true;
        } }

