@{

    bool a = ViewBag.SORTSetCollab;
    bool b = ViewBag.SORTCollab;
    @Html.Hidden("a", a)
    @Html.Hidden("b", b)
}
@{
    bool isAdmin = false;
    int portalType = 0;
    if (Session["UserInfo"] != null)
    {
        var user = (STP.Domain.SecurityAndUsers.UserInfo)Session["UserInfo"];
        portalType = user.UserTypeId;
        if (portalType == 696006 || user.IsAdmin == 1)
        {
            isAdmin = true;
        }
    }

}
<div class="modal fade" id="CollabHistoryDiv" tabindex="-1" role="dialog"
     aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document"
         style="max-width: 60rem;">
        <div class="modal-content" style="width: 60rem;height: auto;">

            @if(ViewBag.LibraryNotes == true)
            {
                Html.RenderAction("LibraryNotesList", "Movements", new { page = 1, pageSize = 10, DocumentId = 0, SORTCollab = ViewBag.SORTCollab });

            }
            else
            {
                if(ViewBag.SORTSetCollab == false)
                {
                    Html.RenderAction("DisplayCollaborationHistoryList", "Notification", new { page = 1, pageSize = 10, DocumentId = ViewBag.DocumentId, SORTCollab = ViewBag.SORTCollab });
                }
                else
                {
                    Html.RenderAction("CreateInternalCollaboration", "Notification", new { SORTSetCollab = ViewBag.SORTSetCollab });
                }
            }


        </div>
        </div>
    </div>
@{
<input type="hidden" value="@ViewBag.SORTSetCollab" id="hf_SORTSetCollab" name="hf_SORTSetCollab" />
}
