<div class="main-table" id="div_reportpermonth">
    <div id="div_tblmonthlyreport_grid" runat="server">
        <table>
            <thead>
                <tr>
                    <th class="first-column-padding">@Resources.Resource.Report_Recieved</th>
                    <th>@Resources.Resource.Report_Accepted</th>
                    <th>@Resources.Resource.Report_Rejected</th>
                    <th>@Resources.Resource.SentforFurtherAssessment</th>
                    <th>@Resources.Resource.NoActionTaken</th>

                </tr>
            </thead>
            <tbody>
                @{
                    foreach (var item in Model)
                    {
                        <tr>
                            <td class="text-color1">@item.ReportRecieved</td>
                            <td class="text-color1">@item.ReportAccepted</td>
                            <td class="text-color1">@item.ReportRejected</td>
                            <td class="text-color1">@item.SentforFurtherAssessment</td>
                            <td class="text-color1">@item.NoActionTaken</td>
                        </tr>
                    }
                }

                @if (Model.Count <= 0)
                {
                    @*<div class="redcolor aligncenter">@Resources.Resource.NoRecordFound</div>*@
                    <tr>
                        <td class="text-color1">0</td>
                        <td class="text-color1">0</td>
                        <td class="text-color1">0</td>
                        <td class="text-color1">0</td>
                        <td class="text-color1">0</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div id="div_export_soareport">
        <button id="btnExportSOAReport" class="btn outline-btn-primary SOAButtonHelper ml2">@Resources.Resource.Export</button>
    </div>
</div>

