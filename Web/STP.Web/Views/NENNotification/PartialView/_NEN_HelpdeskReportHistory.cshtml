<div class="main-table" id="div_categories">
    <div id="div_tblcategory_grid" runat="server">
        <table>
            <thead>
                <tr>
                    <th class="first-column-padding">@Resources.Resource.NEN_CategoriesList</th>
                    <th>@Resources.Resource.NEN_Failures</th>
                    <th>@Resources.Resource.NEN_sent_by_Email</th>
                    <th> NEN via API</th>
                    <th>@Resources.Resource.Total_NEN_Submitted</th>

                </tr>
            </thead>
            <tbody>
                @{
                    foreach (var item in Model)
                    {
                        <tr>
                            <td class="text-color1">@item.Categories</td>
                            <td class="text-color1">@item.NENFailures</td>
                            <td class="text-color1">@item.NENSentByEmail</td>
                            <td class="text-color1">@item.NENSentByApi</td>
                            <td class="text-color1">@item.TotalNENSubmitted</td>
                        </tr>
                    }
                }
            </tbody>
        </table>
        @if (Model.Count <= 0)
        {
            <div class="redcolor aligncenter">@Resources.Resource.NoRecordFound</div>
        }
    </div>
    <div id="div_export_categorylist">
        <button id="btnExportCategory" class="btn outline-btn-primary SOAButtonHelper ml2">@Resources.Resource.Export</button>
    </div>
</div>

<script src="~/Scripts/CspJs/NENNotification\PartialView/_NEN_HelpdeskReportHistory.js?V@(Session["ProjectVersion"])"></script>
